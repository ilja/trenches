FI"G(function() {
  jQuery(function() {
    var sortable_settings;
    new App({
      el: $('#app')
    });
    $('#sortable').sortable({
      update: function() {
        return $.ajax({
          type: 'post',
          data: $('#sortable').sortable('serialize', {
            attribute: 'id'
          }),
          dataType: 'script',
          complete: function() {
            return $('#storylist').effect('highlight');
          },
          url: $('#sortable').attr("data-url")
        });
      }
    });
    $('#sortable').disableSelection();
    $('div.expand').live('click', function() {
      var storybody;
      storybody = $(this).parents("li.story").children('.storybody');
      if (storybody.css('display') === 'none') {
        $(this).find("img").attr({
          src: "/assets/close.png"
        });
      } else {
        $(this).find("img").attr({
          src: "/assets/open.png"
        });
      }
      return storybody.toggle('fast');
    });
    sortable_settings = {
      items: "li:not(.not-sortable)",
      connectWith: ".connectedSortable"
    };
    $(".sortable").sortable(sortable_settings).disableSelection();
    return $(".sortable").sortable({
      update: function(event, ui) {
        return $.ajax({
          type: 'post',
          data: $(this).sortable('serialize', {
            attribute: 'id'
          }),
          dataType: 'script',
          context: this,
          complete: function(request) {
            return $(this).effect('highlight');
          },
          url: $(this).attr('data-url')
        });
      }
    });
  });
}).call(this);
